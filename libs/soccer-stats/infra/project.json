{
  "name": "soccer-stats-infra",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/soccer-stats/infra/src",
  "projectType": "library",
  "tags": ["type:lib", "domain:sports", "scope:soccer-stats"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/soccer-stats/infra",
        "main": "libs/soccer-stats/infra/pulumi.ts",
        "tsConfig": "libs/soccer-stats/infra/tsconfig.build.json",
        "assets": []
      }
    },
    "up": {
      "executor": "nx:run-commands",
      "parallelism": false,
      "options": {
        "command": "pulumi up --yes",
        "cwd": "libs/soccer-stats/infra"
      },
      "dependsOn": ["^build", "build"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "parallelism": false,
      "options": {
        "command": "pulumi preview",
        "cwd": "libs/soccer-stats/infra"
      },
      "dependsOn": ["^build", "build"]
    },
    "refresh": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pulumi refresh --yes",
        "cwd": "libs/soccer-stats/infra"
      }
    },
    "import": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pulumi import",
        "cwd": "libs/soccer-stats/infra"
      }
    }
  }
}
